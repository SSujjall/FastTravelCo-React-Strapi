{
  "kind": "collectionType",
  "collectionName": "destinations",
  "info": {
    "singularName": "destination",
    "pluralName": "destinations",
    "displayName": "Destination",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Location": {
      "type": "string"
    },
    "NumberOfGuests": {
      "type": "integer"
    },
    "NumberOfBedrooms": {
      "type": "integer"
    },
    "NumberOfBathrooms": {
      "type": "integer"
    },
    "PricePerNight": {
      "type": "biginteger"
    },
    "Description": {
      "type": "text"
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "destination"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "destination"
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "House",
        "Hotel",
        "Villa",
        "Apartment",
        "CampHouse"
      ]
    }
  }
}
